{
  "manifest_version": 2,
  "name": "ADD_NAME_HERE",
  "description": "ADD DESCRIPTION HERE",
  "version": "1.0.0",
  "background": {
    "scripts": ["jquery.js", "background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["jquery.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "ADD DEFAULT TITLE",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "web_accessible_resources": []
}
